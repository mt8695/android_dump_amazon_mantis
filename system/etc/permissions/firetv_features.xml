<permissions>
    <!-- By declaring the feature here
         The api PackageManager.hasSystemFeature(String feature) will
         return true if passed the feature name below.-->
    <feature name="com.amazon.feature.contextual_screensaver"/>
    <feature name="com.amazon.feature.dynamic_key_mapping"/>
    <feature name="com.amazon.feature.no_avrcp_vol_control"/>

    <!-- Feature to indicate the starboard is not enabled on the device -->
    <feature name="com.amazon.feature.no_starboard_enabled"/>
</permissions>
